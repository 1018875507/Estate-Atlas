nodes:
  - name: create_database_ods_table
    type: command
    config:
      command: ssh hadoop@master "hive -f /usr/local/software/sql_data/ods_api_regional_housing_prices_i_d.sql" && ssh hadoop@master "hive -f /usr/local/software/sql_data/ods_api_completed_area_of_major_cities_i_d.sql" && ssh hadoop@master "hive -f /usr/local/software/sql_data/ods_api_annual_average_price_i_d.sql" && ssh hadoop@master "hive -f /usr/local/software/sql_data/ods_api_annual_total_sales_area_i_d.sql" && ssh hadoop@master "hive -f /usr/local/software/sql_data/ods_api_annual_total_sales_revenue_i_d.sql" && ssh hadoop@master "hive -f /usr/local/software/sql_data/ods_api_assets_liabilities_i_d.sql" && ssh hadoop@master "hive -f /usr/local/software/sql_data/ods_api_business_operations_i_d.sql" && ssh hadoop@master "hive -f /usr/local/software/sql_data/ods_api_constructed_area_of_major_cities_i_d.sql" && ssh hadoop@master "hive -f /usr/local/software/sql_data/ods_api_regional_housing_sales_area_i_d.sql" && ssh hadoop@master "hive -f /usr/local/software/sql_data/ods_api_regional_housing_sales_revenue_i_d.sql"

  - name: create_database_dwd
    type: command
    dependsOn:
      - create_database_ods_table
    config:
      command: ssh hadoop@master "hive -f /usr/local/software/sql_data/dwd_create_load.sql"


  - name: create_database_dws
    type: command
    dependsOn:
      - create_database_dwd
    config:
      command: ssh hadoop@master "hive -f /usr/local/software/sql_data/dws_create_load.sql"
  

  - name: create_database_ads
    type: command
    dependsOn:
      - create_database_dws
    config:
      command: ssh hadoop@master "hive -f /usr/local/software/sql_data/ads_api_database.sql"

