{
    "job": {
        "setting": {
            "speed": {
                "channel": 3
            }
        },
        "content": [
            {
                "reader": {
                    "name": "hdfsreader",
                    "parameter": {
                        "path": "/house/data/ads/house_complete_rate/*",
                        "defaultFS": "hdfs://master:9000",
                        "column": [
                               {
                                "index": 0,
                                "type": "string"
                               },
                               {
                                "index": 1,
                                "type": "string"
                               },
                               {
                                "index": 2,
                                "type": "double"
                               },
                               {
                                "index": 3,
                                "type": "double"
                               },
                               {
                                "index": 4,
                                "type": "double"
                               }
                        ],
                        "fileType": "text",
                        "encoding": "UTF-8",
                        "fieldDelimiter": ","
                    }

                },
                "writer": {
                    "name": "mysqlwriter",
                    "parameter": {
                        "writeMode": "insert",
                        "username": "root",
                        "password": "123456",
                        "column": [
                            "city",
                            "year",
                            "constructed",
                            "completed",
                            "rate"
                        ],
                        "preSql": [
                            "delete from house_complete_rate"
                        ],
                        "connection": [
                            {
                                "jdbcUrl": "jdbc:mysql://127.0.0.1:3306/house?useUnicode=true&characterEncoding=UTF-8",
                                "table": [
                                    "house_complete_rate"
                                ]
                            }
                        ]
                    }
                }
            }
        ]
    }
}